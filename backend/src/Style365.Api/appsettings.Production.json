{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Authentication": {
    "Cognito": {
      "Region": "",
      "UserPoolId": "",
      "UserPoolClientId": "",
      "Authority": ""
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Error",
        "Style365": "Information",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "/app/logs/app-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithEnvironmentName"]
  },
  "ApplicationUrls": {
    "Api": "https://api.style365.com",
    "CustomerFrontend": "https://www.style365.com",
    "AdminFrontend": "https://admin.style365.com"
  },
  "Features": {
    "EnableSwagger": false,
    "EnableDetailedErrors": false,
    "EnableSensitiveDataLogging": false
  },
  "AllowedHosts": "api.style365.com"
}